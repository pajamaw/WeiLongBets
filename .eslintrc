{
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "jsx-a11y/href-no-hash": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "arrow-parens": "off",
    "no-console": 0,
    "prefer-const": 0,
    "no-undef": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
  }
}
